                    <p class="lead fw-normal mb-0">Your Address List</p>
                  </div>
                  <div class="row g-2" style="justify-content: space-evenly;">
                  <% address.userAddresses.forEach((value) => { %>
                    <!-- <div class="col mb-2"> -->
                      <div class="card mb-4" style="width: 18rem; ">
                        <div class="card-body">
                          <h5 class="card-title"><%= value.fullname%></h5>
                          <h6>mobile:<%= value.mobileNumber %></h6>
                          <p class="card-text"><%= value.houseAddress %> </p>
                          <p class="card-text"><%= value.streetAddress %> ,<%= value.landMark %></p>
                          <p class="card-text"><%= value.cityName %>,<%= value.state %></p>
                          <p class="card-text"><%= value.pincode %></p>
                          <a href="/delete-address/<%= address._id %>/<%= value._id %>" class="btn btn-outline-dark">Delete<i class="fa-solid fa-trash"></i></a>
                          <a href="/edit-address/<%= address._id %>/<%= value._id %>" class="btn btn-outline-dark">Edit<i class="fa-solid fa-pen-to-square"></i></a>
                          
                        </div>
