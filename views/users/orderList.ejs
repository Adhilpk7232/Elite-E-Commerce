<section class="">
  <div class="container">
    <div class="cart_inner">
      
      <div class="table-responsive">
         
        <table class="table" id="">
          <thead>
            <tr>
              <th>SL.No</th>
              
              <th scope="col">Name</th>
              <th scope="col">Address</th>
              <th scope="col">Amount</th>
              <th scope="col">status</th>
              <th scope="col">Action</th>
              <th scope="col">View</th>
              
            </tr>
          </thead>
          <tbody>
              <%if(orders.length == 0) {%>
                  <h3>no orders</h3>
              <%}else{%>
                <% for( let i = 0; i < orders.length; i++ ) { %> 
            <tr>
              <td><%= i + 1 %></td>
              
              
              <td>
                <% orders[i].items.forEach((element,index) => { %>
                <h4><%= element.productId.product_name %>,</h4>
                <% } )%>
              </td>
              
              
              <td>
                <p><%=orders[i].address.fullName %><br><%=orders[i].address.mobileNumber %><br><%=orders[i].address.pincode %><br>
                  <%=orders[i].address.houseAddress %><br><%=orders[i].address.streetAddress %><%=orders[i].address.landMark%>,<%=orders[i].address.cityName %>,<%=orders[i].address.state %>
              </p>
              </td>
              <td>
                <h5><%= orders[i].totalAmount %></h5>
              </td>
              <td><h4><%= orders[i].orderStatus%></h4></td>
              <td class="quantity-col">
                <% if(orders[i].orderStatus == 'cancelled'||orders[i].orderStatus == 'delivered'){%>

                  <%}else{%>
                    <div class="">
                      <a href="/cancel?id=<%=orders[i]._id%> " class="btn_3">Cancel Order</a>
                      
                    </div>
  
                    <%}%>
                    
                      <!-- <div class="">
                        <a href="/return-order " class="btn_3">return Order</a>
                        
                      </div> -->
                    

                
                <!-- End .cart-product-quantity -->
                
            </td>
            <td><a href="/ordered-products?id=<%= orders[i]._id %>"><i class="fa-solid fa-eye"></i></a></td>
            
            </td>
           

            
            </tr>
            </tbody>
            
            <% } 
          }%>
            </table>
            

        </div>
      </div>
    
    </div>
</section>



