    <table class="table" id="productTables">
      <thead>
        <tr>
          <th>Sl.No</th>
          <th>Name</th>
          <th>description</th>
          <th>Image</th>
        </tr>
      </thead>
      <tbody>
        <% for (var i = 0; i < categoryData.length; i++) { %>
          <tr>
            <td><%= i+1 %></td>
            <td><%= categoryData[i].categoryName %></td>
            <td><%= categoryData[i].description %></td> 
            <td><img src="/category-img/<%= categoryData.image %>" alt="" style="width: 80px; height: 80px;"></td>
            <td>
         <a href="/admin/deleteCategory?id=<%= categoryData[i]._id %>"><button class="btn btn-danger mr-2" onclick="click()">Delete</button></a>     
           <a href="/admin/updateCategory?id=<%= categoryData[i]._id %>"><button class="btn btn-success">Update</button></a>   
            </td>
          </tr>
        <% } %>
        
